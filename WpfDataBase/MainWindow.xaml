<Window x:Class="WpfDataBase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDataBase"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="datagrid" HorizontalAlignment="Left" Height="292" Margin="31,52,0,0" VerticalAlignment="Top" Width="386"
                  ItemsSource="{Binding}"/>
        <ListBox x:Name="listbox" HorizontalAlignment="Left" Height="292" Margin="456,52,0,0" VerticalAlignment="Top" Width="295"
                 ItemsSource="{Binding}"/>



    </Grid>
</Window>
